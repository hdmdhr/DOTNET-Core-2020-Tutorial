@model System.Collections.Generic.IEnumerable<empty_project.Models.Employee>

@{
    Layout = "_Layout";
    ViewBag.Title = "Employee List";
}

    <div>
        <table>
            <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
            </thead>
            <tbody>
                @foreach (var employee in Model)
                {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.Name</td>
                    <td>@employee.Department</td>
                </tr>
                }
            </tbody>
       
        </table>
    </div>